{
  "name": "soytuagente-workspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run start:web",
    "start:web": "nx serve web --host 0.0.0.0 --port 4200 --proxy-config web/proxy.conf.json",
    "build:web": "nx build web",
    "build:web:prod": "ng build --configuration production",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "prebuild:api": "npm run prisma:generate",
    "build:api": "tsc -p api/tsconfig.build.json",
    "start:api": "node api/dist/main.js",
    "dev:api": "nodemon --watch api/src --ext ts --exec \"tsx -r tsconfig-paths/register api/src/main.ts\"",
    "pm2:start:api": "pm2 start api/dist/main.js --name api --cwd ./ --time",
    "pm2:restart:api": "npm run build:api && pm2 restart api",
    "pm2:stop:api": "pm2 stop api || true",
    "pm2:logs:api": "pm2 logs api --lines 80",
    "build:all": "npm run build:web:prod && npm run build:api",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@editorjs/editorjs": "^2.31.0",
